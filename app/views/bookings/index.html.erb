<div class="container">
  <div class="row">
  <h1>All your bookings</h1>
  <% @bookings.each do |booking| %>
    <div class="col-xs-12 col-sm-12">
      <h2>link_to booking.tool.name, booking_path(booking)</h2>
        <%= link_to "Delete booking", booking_path(booking), method: :delete %>
    </div>
  <% end %>
  </div>
</div>

